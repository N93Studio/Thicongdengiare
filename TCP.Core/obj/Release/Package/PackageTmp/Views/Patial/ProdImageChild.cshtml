
@{
    Layout = null;
}
@if (Model != null)
{
<div class="row">
    @foreach (var item in Model)
    {
        <div class="col-sm-6">
            <div class="items mbchildimg">
                <img src="~/Upload/ProductsImg/@item.Images" class="img-responsive" data-aos="fade-up" data-aos-duration="1000"/>
            </div>
        </div>
    }
</div>
    
}